{
  "name": "specify-api-server",
  "version": "0.0.1",
  "description": "Api server for Specify application",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=dev npm-run-all --parallel dev:*",
    "dev:build": "webpack --config webpack.config.js --watch",
    "dev:watch": "nodemon --watch build --exec \"node build/bundle.js\" ",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register --require mocha-graphql-register --recursive",
    "lint": "eslint --ignore-pattern '/build/bundle.js' **/*.js"
  },
  "author": "Taranenko",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.7.1",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.18.0",
    "jwt-simple": "^0.5.0",
    "lodash": "4.17.4",
    "mongoose": "^4.13.9",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.1.0",
    "mocha-graphql-register": "^1.0.0",
    "raw-loader": "^0.5.1",
    "supertest": "^3.0.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
